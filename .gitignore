# Ignore bundler config.
/.bundle

# Ignore the default SQLite database.
/db/*.sqlite3
/db/*.sqlite3-journal
/db/data.yml

# Ignore all logfiles and tempfiles.
/log/*.log
/tmp

/config/*.yml
!/config/business_time.yml

/.idea/
/public/spree
/public/assets
/private

# Ignore application configuration
/config/application.yml

# Ignore vim swp files
*.swp

# Whatever that is
log/sunspot-solr-development.log.lck
# Ignore local solr stuff
/solr
/.DS_Store
/**/.DS_Store

# Gemfile uses sensitive information, so don't check in the lockfile
Gemfile.lock
